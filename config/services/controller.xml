<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <defaults public="true" />

        <service id="sylius_notification.action.get_updated_plugins" class="Sylius\NotificationSenderPlugin\Action\GetUpdatedPluginsAction">
            <argument type="service" id="sylius_notification.packages_registry" />
            <argument type="service" id="sylius_notification.package_versions_provider.packagist" />
        </service>
    </services>
</container>
